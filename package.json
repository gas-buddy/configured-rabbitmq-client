{
  "name": "@gasbuddy/configured-rabbitmq-client",
  "version": "6.0.1",
  "description": "A configuration driven RabbitMQ client",
  "main": "build/index.js",
  "scripts": {
    "test": "tap --node-arg=-r --node-arg=@babel/register tests/*.js",
    "test-some": "tap --node-arg=-r --node-arg=@babel/register",
    "cover": "cross-env BABEL_ENV=test nyc tap tests/test_*.js && nyc check-coverage && nyc report --reporter=text",
    "lint": "eslint .",
    "build": "babel src --out-dir build",
    "prepublish": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:gas-buddy/gb-services.git/configured-rabbitmq-client.git"
  },
  "keywords": [
    "rabbitmq",
    "rabbot",
    "gasbuddy"
  ],
  "author": "GasBuddy",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.1.2",
    "@babel/register": "^7.0.0",
    "babel-preset-gasbuddy": "^4.1.1",
    "bluebird": "^3.5.3",
    "cross-env": "^5.2.0",
    "eslint-config-gasbuddy": "^5.0.2",
    "tap": "^12.0.1"
  },
  "dependencies": {
    "lodash": "^4.17.11",
    "rabbot": "^2.1.0"
  },
  "nyc": {
    "include": [
      "src"
    ],
    "require": [
      "@babel/register"
    ],
    "sourceMap": false,
    "instrument": false,
    "reporter": [
      "lcov"
    ]
  }
}
